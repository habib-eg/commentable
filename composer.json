{
    "name": "habib/commentable",
    "description": "laravel comment package",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "keywords": ["laravel", "comment"],
    "authors": [
        {
            "name": "mohamedhabib",
            "email": "mohamedhabibwork@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "laravel/framework": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "orchestra/testbench": ">=4.0"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "Habib\\Commentable\\": "src/"
        },
        "files": [
            "./src/helper/main.php"
        ],
        "classmap": [
            "./src/database/factory"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Habib\\Commentable\\Providers\\CommentableServiceProvider"
            ]
        }
    }
}
